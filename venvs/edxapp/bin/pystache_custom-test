#!/edx/app/edxapp/venvs/edxapp/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pystache-custom','console_scripts','pystache_custom-test'
__requires__ = 'pystache-custom'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pystache-custom', 'console_scripts', 'pystache_custom-test')()
    )
